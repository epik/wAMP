}

/*----------------
  This is for the knob
  ----------------*/

.rotateable
{
	position:absolute;
	overflow:hidden;
	-webkit-transition-property: -webkit-transform; 
	-webkit-transition-duration: 0s;
}

.rotateablefront
{
	position:absolute;
	z-index:3000;
	overflow:hidden;
	-webkit-transition-property: -webkit-transform; 
	-webkit-transition-duration: 0s;
}

.rotateableback
{
	position:absolute;
	z-index:2003;
	overflow:hidden;
	-webkit-transition-property: -webkit-transform; 
	-webkit-transition-duration: 0s;
}

.rotateablebackfill
{
	position:absolute;
	z-index:2030;
	overflow:hidden;
	-webkit-transition-property: -webkit-transform; 
	-webkit-transition-duration: 0s;
	background:url(display.png) 0 0 no-repeat;
}

.rotateabledisplay
{
	position:absolute;
	z-index:2300;
	overflow:hidden;
	-webkit-transition-property: -webkit-transform; 
	-webkit-transition-duration: 0s;
}

.rotateableparent
{
	position:absolute;
	z-index:2000;
	overflow:hidden;
	-webkit-transition-property: -webkit-transform; 
	-webkit-transition-duration: 0s;
}

.rotateablelbl
{
	position:absolute;
	z-index: 3000;
	top: 10px;
	width: 100%;
	font-weight: bold;
	font-size: 20px;
	color: #000;
	text-align: center;
}

/*----------------
  This is for the spinner
  See wAMP.js for the required notice under the MIT license
  ----------------*/

#sw-wrapper {
	position:absolute; z-index:1000;
	left:0;
	width:100%;
	font-family:helvetica, sans-serif;
	background:rgba(0,0,0,0.7);
	text-align:left;
}

#sw-header {
	position:relative;
	width:100%; height:43px;
	border-top:1px solid #000; border-bottom:1px solid #000;
	background:url(../res/spinner/sw-header.png) 0 0 repeat-x;
	opacity: 0.9;
}

#sw-cancel, #sw-done {
	position:absolute;
	top:7px;
	height:20px; line-height:20px;
	padding:0 5px; margin:0;
	border-width:5px;
	font-size:12px; font-weight:bold;
	text-shadow:rgba(0,0,0,0.8) 0 -1px 0;
	color:#fff;
}

#sw-cancel {
	left:7px;
	float:left;
	-webkit-border-image:url(../res/spinner/sw-button-cancel.png) 5;
}

#sw-done {
	right:7px;
	float:right;
	-webkit-border-image:url(../res/spinner/sw-button-done.png) 5;
}

.sw-pressed { opacity:0.4; }

#sw-slots-wrapper {
	position:relative; z-index:999;
	display:block;
	height:127px;
	padding:0 11px;
	overflow:hidden;
}

#sw-slots {
	display:table;
	width:100%;
	background:#fcfcfc;
}

#sw-slots div {
	display:table-cell;
	height:100%;
	padding-top:42px;
	border-left:2px solid #0d0e0f;
	background-color:#fcfcfc;
	background-image: url(../res/spinner/sw-slot-border.png);
	background-position: 0 0, 100% 0;
	background-repeat: repeat-y;
}

#sw-slots div:first-child { border:0; }

#sw-slots ul {
	padding:0 0 85px 0; margin:0;
	list-style:none;
}

#sw-slots .sw-right { text-align:right; }
#sw-slots .sw-shrink { width:1%; }
#sw-slots .sw-readonly { background:#ddd; }

#sw-slots li {
	padding:0 8px;
	height:44px;
	overflow:hidden;
	font:bold 24px/44px Helvetica,sans-serif;
}

#sw-frame {
	position:absolute; z-index:1000;
	left:0; right:0; top:43px;
	height:225px;
	border-width:16px;
	-webkit-border-image:url(../res/spinner/sw-alpha.png) 16;
}


/*****************************
 * CSS routines for the Record spin
 *****************************/
.recordwrapper
{
position: absolute;
/*0000af*/
}


.recordparent
{
	position: absolute;
	background-image:url('../res/record/record.png');
	z-index: 6;
	height: 200px;
	width: 200px;
}

.recordhole
{
	position: absolute;
	background-image:url('../res/record/secondcenter.png');
	z-index: 1000;
	height: 200px;
	width: 200px;
}

.recordspin
{
	position: absolute;
	background-image:url('../res/record/firstcenter.png');
	z-index: 60;
	height: 200px;
	width: 200px;
}

.recordpic
{
	position: absolute;
	background-image:url('../res/record/defaultcov.png');
	z-index: 600;
	height: 44px;
	width: 44px;
}

.recordpicspin
{
	position: absolute;
	background-image:url('../res/record/defaultcov.png');
	z-index: 600;
	height: 44px;
	width: 44px;
  -webkit-animation-name: rotateThis;
  -webkit-animation-duration:2s;
  -webkit-animation-iteration-count:infinite;
  -webkit-animation-timing-function:linear;
}
@-webkit-keyframes rotateThis {
  from {-webkit-transform: rotate(0deg);}
  to {-webkit-transform: rotate(360deg);}
}